<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="@color/transparent">
	
	<androidx.constraintlayout.widget.ConstraintLayout
			android:layout_width="0dp"
			android:layout_height="100dp"
			android:background="@drawable/circle_round_edge_white"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintBottom_toBottomOf="parent"
			android:layout_marginStart="40dp"
			android:layout_marginEnd="32dp"
			android:layout_marginTop="8dp"
			android:layout_marginBottom="8dp"
			android:id="@+id/constraintLayout3">
		
		<TextView
				android:text="TextView"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="@color/bottom_menu_color"
				android:textSize="@dimen/font_size_s3"
				android:textStyle="bold"
				android:id="@+id/travel_name_tv"
				app:layout_constraintStart_toStartOf="parent"
				android:layout_marginStart="62dp"
				app:layout_constraintTop_toTopOf="parent"
				android:layout_marginTop="16dp"/>
		
		<TextView
				android:text="TextView"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/travel_memo_tv"
				android:textColor="@color/gray"
				android:textSize="@dimen/font_size_s4"
				app:layout_constraintStart_toStartOf="parent"
				android:layout_marginStart="62dp"
				android:layout_marginTop="4dp"
				app:layout_constraintTop_toBottomOf="@+id/travel_name_tv"/>
		
		<ImageView
				android:layout_width="15dp"
				android:layout_height="15dp"
				app:srcCompat="@mipmap/users"
				android:id="@+id/imageView12"
				app:layout_constraintStart_toStartOf="parent"
				android:layout_marginStart="62dp"
				app:layout_constraintTop_toBottomOf="@+id/travel_memo_tv"
				android:layout_marginTop="8dp"/>
		
		<TextView
				android:text="TextView"
				android:textColor="@color/bottom_menu_color"
				android:textSize="@dimen/font_size_s4"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/travel_person_number"
				app:layout_constraintBottom_toBottomOf="@+id/imageView12"
				app:layout_constraintStart_toEndOf="@+id/imageView12"
				android:layout_marginStart="5dp"
				/>
		
		<ImageView
				android:layout_width="15dp"
				android:layout_height="15dp"
				app:srcCompat="@mipmap/money"
				android:id="@+id/imageView13"
				app:layout_constraintStart_toEndOf="@+id/travel_person_number"
				android:layout_marginStart="16dp"
				app:layout_constraintTop_toBottomOf="@+id/travel_memo_tv"
				android:layout_marginTop="8dp"/>
		
		<TextView
				android:text="TextView"
				android:textColor="@color/red"
				android:textSize="@dimen/font_size_s4"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/travel_money_tv"
				app:layout_constraintStart_toEndOf="@+id/imageView13"
				app:layout_constraintTop_toTopOf="@+id/imageView13"
				app:layout_constraintBottom_toBottomOf="@+id/imageView13"
				android:layout_marginStart="5dp"/>
		
		<TextView
				android:text="2019年12月12日"
				android:textColor="@color/gray"
				android:textSize="@dimen/font_size_s5"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/travel_date_tv"
				app:layout_constraintStart_toEndOf="@+id/travel_name_tv"
				android:layout_marginStart="8dp"
				app:layout_constraintBottom_toBottomOf="@+id/travel_name_tv"/>
	
	</androidx.constraintlayout.widget.ConstraintLayout>
	
	<hll.zpf.starttravel.common.components.CRImageView
			android:layout_width="70dp"
			android:layout_height="70dp"
			android:src="@mipmap/simple"
			android:id="@+id/travel_image"
			app:layout_constraintTop_toTopOf="@+id/constraintLayout3"
			app:layout_constraintBottom_toBottomOf="@+id/constraintLayout3"
			app:layout_constraintStart_toStartOf="parent"
			app:radius="5dp"
			android:layout_marginStart="20dp"/>
	
	<Button
			android:text="@string/history_009"
			android:layout_width="70dp"
			android:layout_height="30dp"
			android:id="@+id/travel_detail_bt"
			android:textColor="@color/white"
			android:textStyle="bold"
			android:textSize="@dimen/font_size_s4"
			android:background="@drawable/button_background7"
			android:layout_marginTop="60dp"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="16dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>
