<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="@drawable/circle_round_edge_white_10"
		tools:context=".page.fragment.TravelFragment">
	
	<hll.zpf.starttravel.common.components.CRImageView
			android:src="@mipmap/welcome"
			android:layout_width="0dp"
			android:layout_height="150dp"
			android:id="@+id/travel_image"
			android:scaleType="centerCrop"
			app:left_top_radius="10dp"
			app:right_top_radius="10dp"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			/>
	
	<TextView
			android:layout_width="0dp"
			android:layout_height="50dp"
			android:id="@+id/textView12"
			android:background="@drawable/bottom_circle_round"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			/>
	
	<TextView
			android:text="TextView"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:id="@+id/travel_name_tv"
			android:gravity="center"
			android:textSize="@dimen/font_size_s1"
			android:textStyle="bold"
			android:textColor="@color/bottom_menu_color"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintHorizontal_bias="0.5"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="5dp"
			android:layout_marginStart="5dp"
			android:layout_marginTop="3dp"
			app:layout_constraintTop_toBottomOf="@+id/travel_image"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toTopOf="@+id/travel_action_bt"/>
	
	<TextView
			android:text="2019/12/17 12:34"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/travel_start_date"
			android:textColor="@color/gray"
			android:textSize="@dimen/font_size_s5"
			app:layout_constraintStart_toStartOf="@+id/travel_name_tv"
			app:layout_constraintEnd_toEndOf="@+id/travel_name_tv"
			android:layout_marginTop="3dp"
			app:layout_constraintTop_toBottomOf="@+id/travel_name_tv"/>
	
	<Button
			android:text="@string/travel_001"
			android:layout_width="80dp"
			android:layout_height="40dp"
			android:id="@+id/travel_action_bt"
			android:textSize="@dimen/font_size_s3"
			android:textColor="@color/white"
			android:background="@drawable/button_background3"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintHorizontal_bias="0.5"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginTop="8dp"
			app:layout_constraintTop_toBottomOf="@+id/travel_image"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toTopOf="@+id/textView12"/>
	
	<TextView
			android:text="@string/travel_003"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:id="@+id/travel_memo_tv"
			android:textColor="@color/bottom_menu_color"
			android:textSize="@dimen/font_size_s4"
			android:gravity="center"
			android:layout_marginTop="8dp"
			app:layout_constraintTop_toBottomOf="@+id/travel_action_bt"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="5dp"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginStart="5dp"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toTopOf="@+id/textView22"/>
	
	<TextView
			android:text="1,000"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/travel_money_tv"
			android:textColor="@color/red"
			android:textSize="@dimen/font_size_s4"
			android:textStyle="bold"
			android:layout_marginBottom="16dp"
			app:layout_constraintBottom_toTopOf="@+id/textView12"
			app:layout_constraintStart_toEndOf="@+id/textView22"
			/>
	
	<TextView
			android:text="@string/travel_004"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView22"
			android:textColor="@color/red"
			android:textSize="@dimen/font_size_s4"
			app:layout_constraintTop_toTopOf="@+id/travel_money_tv"
			app:layout_constraintBottom_toBottomOf="@+id/travel_money_tv"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginStart="24dp"/>
	
	<TextView
			android:text="0äºº"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/travel_person_number_tv"
			android:textColor="@color/bottom_menu_color"
			android:textSize="@dimen/font_size_s4"
			android:textStyle="bold"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="24dp"
			android:layout_marginBottom="16dp"
			app:layout_constraintBottom_toTopOf="@+id/textView12"
			/>
	
	<TextView
			android:text="@string/travel_005"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView24"
			android:textColor="@color/bottom_menu_color"
			android:textSize="@dimen/font_size_s4"
			app:layout_constraintTop_toTopOf="@+id/travel_person_number_tv"
			app:layout_constraintBottom_toBottomOf="@+id/travel_person_number_tv"
			app:layout_constraintEnd_toStartOf="@+id/travel_person_number_tv"
			android:layout_marginEnd="3dp"/>
	
	<Button
			android:background="@drawable/edit1_button_background"
			android:layout_width="34dp"
			android:layout_height="34dp"
			android:id="@+id/travel_item_edit"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintStart_toEndOf="@+id/textView12"
			app:layout_constraintHorizontal_bias="0.5"
			app:layout_constraintEnd_toStartOf="@+id/textView12"
			android:layout_marginBottom="8dp"/>
	
	<Button
			android:layout_width="34dp"
			android:layout_height="34dp"
			android:id="@+id/travel_item_flag"
			android:background="@drawable/flag_button_background"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toStartOf="@+id/travel_item_edit"
			android:layout_marginEnd="64dp"
			android:layout_marginBottom="8dp"/>
	
	<Button
			android:background="@drawable/detail_button_background"
			android:layout_width="34dp"
			android:layout_height="34dp"
			android:id="@+id/travel_item_detail"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintStart_toEndOf="@+id/travel_item_edit"
			android:layout_marginStart="64dp"
			android:layout_marginBottom="8dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>