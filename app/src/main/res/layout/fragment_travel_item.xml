<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="@color/transparent"
		tools:context=".page.fragment.TravelFragment">

	<View android:layout_width="match_parent"
		  android:layout_height="0dp"
		  app:layout_constraintTop_toTopOf="parent"
		  app:layout_constraintStart_toStartOf="parent"
		  app:layout_constraintEnd_toEndOf="parent"
		  app:layout_constraintBottom_toBottomOf="parent"
		  android:layout_marginBottom="40dp"
		  android:background="@drawable/circle_round_edge_green_10"
		  android:id="@+id/view3"/>


	<hll.zpf.starttravel.common.components.CRImageView
			android:src="@mipmap/default_image"
			android:layout_width="120dp"
			android:layout_height="150dp"
			android:id="@+id/partner_image"
			android:scaleType="centerCrop"
			app:radius="10dp"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			android:layout_marginTop="24dp"
			android:layout_marginStart="16dp"
	/>

	<TextView
			android:text="TextView"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:id="@+id/travel_name_tv"
			android:gravity="left"
			android:textSize="@dimen/font_size_s2"
			android:textStyle="bold"
			android:textColor="@color/white"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="5dp"
			app:layout_constraintStart_toEndOf="@+id/partner_image"
			android:layout_marginStart="8dp"
			app:layout_constraintBottom_toTopOf="@+id/travel_start_date"/>

	<TextView
			android:text="2019/12/17 12:34"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/travel_start_date"
			android:textColor="@color/white"
			android:textSize="@dimen/font_size_s5"
			android:gravity="left"
			app:layout_constraintStart_toEndOf="@+id/partner_image" android:layout_marginStart="8dp"
			app:layout_constraintTop_toTopOf="@+id/partner_image"
			app:layout_constraintBottom_toBottomOf="@+id/partner_image"/>

	<Button
			android:text="@string/travel_001"
			android:layout_width="50dp"
			android:layout_height="50dp"
			android:id="@+id/travel_action_bt"
			android:textSize="@dimen/font_size_s3"
			android:textStyle="bold"
			android:textColor="@color/white"
			android:stateListAnimator="@null"
			android:background="@drawable/button_background7"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintHorizontal_bias="0.5"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginTop="10dp"
			app:layout_constraintTop_toBottomOf="@+id/travel_item_edit"/>

	<TextView
			android:text="@string/travel_003"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:id="@+id/travel_memo_tv"
			android:textColor="@color/white"
			android:textSize="@dimen/font_size_s3"
			android:gravity="center"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="5dp"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginStart="5dp"
			android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/partner_image"
			android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/travel_item_edit"/>

	<TextView
			android:text="1,000"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/travel_money_tv"
			android:textColor="@color/light_red"
			android:textSize="@dimen/font_size_s0"
			android:textStyle="bold"
			app:layout_constraintStart_toEndOf="@+id/textView22"
			app:layout_constraintTop_toTopOf="@+id/textView22"
			app:layout_constraintBottom_toBottomOf="@+id/textView22"/>

	<TextView
			android:text="@string/travel_004"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView22"
			android:textColor="@color/light_red"
			android:textSize="@dimen/font_size_s4"
			android:textStyle="bold"
			app:layout_constraintStart_toEndOf="@+id/partner_image"
			android:layout_marginStart="8dp" app:layout_constraintTop_toBottomOf="@+id/travel_start_date"
			android:layout_marginTop="8dp"/>

	<TextView
			android:text="0äºº"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/travel_person_number_tv"
			android:textColor="@color/white"
			android:textSize="@dimen/font_size_s3"
			android:textStyle="bold"
			android:layout_marginEnd="16dp"
			app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="16dp"
			app:layout_constraintTop_toTopOf="parent"/>

	<Button
			android:background="@drawable/edit1_button_background"
			android:layout_width="34dp"
			android:layout_height="34dp"
			android:stateListAnimator="@null"
			android:id="@+id/travel_item_edit"
			android:layout_marginBottom="40dp"
			app:layout_constraintBottom_toBottomOf="@+id/view3"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintHorizontal_bias="0.5"
			app:layout_constraintEnd_toEndOf="parent"/>
	<Button
			android:layout_width="34dp"
			android:layout_height="34dp"
			android:id="@+id/travel_item_flag"
			android:stateListAnimator="@null"
			android:background="@drawable/flag_button_background"
			app:layout_constraintEnd_toStartOf="@+id/travel_item_edit"
			android:layout_marginEnd="60dp"
			app:layout_constraintBottom_toBottomOf="@+id/view3" android:layout_marginBottom="40dp"/>

	<Button
			android:background="@drawable/detail_button_background"
			android:layout_width="34dp"
			android:layout_height="34dp"
			android:stateListAnimator="@null"
			android:id="@+id/travel_item_detail"
			app:layout_constraintStart_toEndOf="@+id/travel_item_edit"
			android:layout_marginStart="60dp"
			android:layout_marginBottom="40dp" app:layout_constraintBottom_toBottomOf="@+id/view3"/>
	<View
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:id="@+id/travel_none_background"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintBottom_toBottomOf="parent"
			android:background="@drawable/button_background5"
			android:visibility="gone"/>
	<ImageView
			android:src="@mipmap/welcome"
			android:layout_width="0dp"
			android:layout_height="200dp"
			android:scaleType="center"
			android:id="@+id/travel_none_background_image"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintHorizontal_bias="0.5"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintBottom_toBottomOf="parent"
			android:visibility="gone"/>

</androidx.constraintlayout.widget.ConstraintLayout>