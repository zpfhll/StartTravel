<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="@color/background_color"
		tools:context=".page.fragment.MeFragment">
	
	
	<View
			android:layout_width="wrap_content"
			android:layout_height="170dp"
			android:id="@+id/view"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			android:background="@color/white"
			app:layout_constraintHorizontal_bias="0.0"
			/>
	
	<hll.zpf.starttravel.common.components.CRImageView
			android:layout_width="66dp"
			android:layout_height="66dp"
			android:id="@+id/user_img"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginTop="52dp"
			android:layout_marginStart="25dp"
			app:radius="90dp"
			android:src="@mipmap/ic_launcher"
			/>
	
	<TextView
			android:text="@string/me_001"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView"
			android:textSize="@dimen/font_size_s4"
			android:textColor="@color/light_black"
			app:layout_constraintStart_toEndOf="@+id/user_img"
			android:layout_marginStart="16dp"
			app:layout_constraintTop_toTopOf="@+id/user_img"
			app:layout_constraintBottom_toBottomOf="@+id/user_img"/>
	
	<TextView
			android:text="@string/me_002"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView5"
			android:textSize="@dimen/font_size_s4"
			android:textColor="@color/gray"
			app:layout_constraintTop_toBottomOf="@+id/textView"
			android:layout_marginStart="16dp"
			app:layout_constraintStart_toEndOf="@+id/user_img"
			android:layout_marginTop="3dp"/>
	
	<TextView
			android:text="TextView"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/user_name_tv"
			android:textColor="@color/bottom_menu_color"
			android:textStyle="bold"
			android:textSize="@dimen/font_size_s2"
			android:layout_marginStart="16dp"
			app:layout_constraintStart_toEndOf="@+id/user_img"
			android:layout_marginBottom="3dp"
			app:layout_constraintBottom_toTopOf="@+id/textView"/>
	
	<TextView
			android:text="@string/me_006"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/user_memo_tv"
			android:textSize="@dimen/font_size_s4"
			android:textColor="@color/light_black"
			app:layout_constraintStart_toEndOf="@+id/textView"
			app:layout_constraintTop_toTopOf="@+id/textView"
			app:layout_constraintBottom_toBottomOf="@+id/textView"/>
	
	<TextView
			android:text="TextView"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/user_mail_tv"
			android:textSize="@dimen/font_size_s4"
			android:textColor="@color/gray"
			app:layout_constraintStart_toEndOf="@+id/textView5"
			app:layout_constraintTop_toTopOf="@+id/textView5"
			app:layout_constraintBottom_toBottomOf="@+id/textView5"/>
	
	<View
			android:clickable="true"
			android:background="@drawable/button_background4"
			android:layout_width="wrap_content"
			android:layout_height="60dp"
			android:id="@+id/help_view"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginTop="60dp"
			app:layout_constraintTop_toBottomOf="@+id/view"
			/>
	
	<View
			android:clickable="true"
			android:background="@drawable/button_background4"
			android:layout_width="wrap_content"
			android:layout_height="60dp"
			android:id="@+id/about_view"
			android:layout_marginTop="1dp"
			app:layout_constraintTop_toBottomOf="@+id/help_view"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"/>
	
	<TextView
			android:text="@string/me_003"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView9"
			app:layout_constraintTop_toTopOf="@+id/help_view"
			app:layout_constraintBottom_toBottomOf="@+id/help_view"
			app:layout_constraintStart_toStartOf="@+id/help_view"
			android:layout_marginStart="16dp"
			android:textColor="@color/light_black"
			android:textSize="@dimen/font_size_s3"
			/>
	
	<TextView
			android:text="@string/me_004"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView10"
			app:layout_constraintStart_toStartOf="@+id/about_view"
			android:layout_marginStart="16dp"
			android:textColor="@color/light_black"
			android:textSize="@dimen/font_size_s3"
			app:layout_constraintTop_toTopOf="@+id/about_view"
			app:layout_constraintBottom_toBottomOf="@+id/about_view"
			/>
	
	<View
			android:clickable="true"
			android:layout_width="0dp"
			android:layout_height="60dp"
			android:id="@+id/logout_view"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginTop="24dp"
			app:layout_constraintTop_toBottomOf="@+id/about_view"
			android:background="@drawable/button_background4"
			style="@android:style/Widget.Button"/>
	
	<ImageView
			android:layout_width="15dp"
			android:layout_height="15dp"
			app:srcCompat="@mipmap/right"
			android:id="@+id/imageView6"
			app:layout_constraintTop_toTopOf="@+id/textView9"
			app:layout_constraintBottom_toBottomOf="@+id/textView9"
			app:layout_constraintEnd_toEndOf="@+id/help_view"
			android:layout_marginEnd="24dp"/>
	
	<ImageView
			android:layout_width="15dp"
			android:layout_height="15dp"
			app:srcCompat="@mipmap/right"
			android:id="@+id/imageView8"
			app:layout_constraintTop_toTopOf="@+id/textView10"
			app:layout_constraintBottom_toBottomOf="@+id/textView10"
			app:layout_constraintEnd_toEndOf="@+id/about_view"
			android:layout_marginEnd="24dp"/>
	
	<TextView
			android:text="@string/me_005"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView6"
			android:textColor="@color/red"
			android:textStyle="bold"
			android:textSize="@dimen/font_size_s2"
			app:layout_constraintTop_toTopOf="@+id/logout_view"
			app:layout_constraintBottom_toBottomOf="@+id/logout_view"
			app:layout_constraintStart_toStartOf="@+id/logout_view"
			app:layout_constraintEnd_toEndOf="@+id/logout_view"/>
	
	<Button
			android:layout_width="34dp"
			android:layout_height="15dp"
			android:id="@+id/edit_name_btn"
			app:layout_constraintStart_toEndOf="@+id/user_name_tv"
			android:layout_marginStart="8dp"
			android:background="@drawable/edit_button_background"
			android:layout_marginBottom="5dp"
			app:layout_constraintBottom_toTopOf="@+id/user_memo_tv"/>
	
	<Button
			android:layout_width="34dp"
			android:layout_height="15dp"
			android:id="@+id/edit_memo_btn"
			android:background="@drawable/edit_button_background"
			app:layout_constraintTop_toTopOf="@+id/user_img"
			app:layout_constraintBottom_toBottomOf="@+id/user_img"
			app:layout_constraintStart_toEndOf="@+id/user_memo_tv"
			android:layout_marginStart="8dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>