<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/circle_round_edge"
		tools:context=".page.HomeActivity">
	
	<TextView
			android:layout_width="0dp"
			android:layout_height="50dp"
			android:id="@+id/list_dialog_title"
			android:textSize="@dimen/font_size_s2"
			android:textStyle="bold"
			android:gravity="center"
			android:textColor="@color/white"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			android:background="@drawable/top_circle_round_title"/>
	
	<ListView
			android:layout_width="300dp"
			android:layout_height="280dp"
			app:layout_constraintTop_toBottomOf="@+id/list_dialog_title"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			android:id="@+id/content_list"
			android:background="@color/white"
			app:layout_constraintBottom_toTopOf="@+id/list_dialog_cancel"
			/>
	
	<Button
			android:text="@string/list_dialog_001"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:id="@+id/list_dialog_cancel"
			android:textSize="@dimen/font_size_s3"
			android:textStyle="bold"
			android:textColor="@color/white"
			android:background="@drawable/button_background6"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginStart="16dp"
			android:layout_marginEnd="16dp"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/content_list"
			android:layout_marginTop="8dp"
			android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>